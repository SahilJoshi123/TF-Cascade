<!DOCTYPE html>
<html>
  <head>
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no">
    <meta charset="utf-8">
    <title>Marker Labels</title>
    <style>
      /* Always set the map height explicitly to define the size of the div
       * element that contains the map. */
      #map {
        height: 100%;
      }
      /* Optional: Makes the sample page fill the window. */
      html, body {
        height: 100%;
        margin: 0;
        padding: 0;
      }
    </style>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCHOfSWStEXXxGlP8g-sdaEJLdCSydnDPo"></script>
    <script>
      // In the following example, markers appear when the user clicks on the map.
      // Each marker is labeled with a single alphabetical character.
      //var labels = 'ABCDEFGHIJKLMNOPQRSTUVWXYZ';
      var labelIndex = 0;

      function initialize() {
        var bangalore = { lat: 12.97, lng: 76.20, name:'Place 8', icon:'images/l2.png', patients:110 };
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 7,
          center: {lat: 14.97, lng: 76.20}
        });

        // This event listener calls addMarker() when the map is clicked.
        //google.maps.event.addListener(map, 'click', function(event) {
        //  addMarker(event.latLng, map);
        //});

        // Add a marker at the center of the map.
        addMarker(bangalore, map);
        var c2 = { lat: 11.971, lng: 76.593, name:'place - 1', icon:'images/l1.png', patients:100 };
        addMarker(c2, map);
        var c3 = { lat: 12.973, lng: 74.997, name:'place - 2', icon:'images/s1.png', patients:20 };
        addMarker(c3, map);
        var c4 = { lat: 12.976, lng: 77.590, name:'place - 3', icon:'images/s4.png', patients:25 };
        addMarker(c4, map);
        var c5 = { lat: 16.978, lng: 76.578, name:'place - 4', icon:'images/l3.png', patients:120 };
        addMarker(c5, map);
        var c6 = { lat: 13.979, lng: 76.579, name:'place - 5', icon:'images/s5.png', patients:30 };
        addMarker(c6, map);
        var c7 = { lat: 14.975, lng: 76.575, name:'place - 6', icon:'images/l5.png', patients:105 };
        addMarker(c7, map);
        var c8 = { lat: 14.974, lng: 75.574, name:'place - 7', icon:'images/s4.png', patients:10 };
        addMarker(c8, map);
        var c8 = { lat: 13.974, lng: 74.574, name:'place - 9', icon:'images/s4.png', patients:10 };
        addMarker(c8, map);
        
      }
      
      var infowindow = new google.maps.InfoWindow({
          content: "<div>kkkkkkk</div>"
        });
      
      // Adds a marker to the map.
      function addMarker(location, map) {
        // Add the marker at the clicked location, and add the next-available label
        // from the array of alphabetical characters.
        var marker = new google.maps.Marker({
          position: location,
          label: location['name'],
          icon: location['icon'],
          patients: location['patients'],
          map: map
        });
        marker.addListener('click', function() {
          infowindow.setContent('<div><table><tr><td><img src='
                                +marker.icon+' /></td><td><strong>Name:' +
                                marker.label + '<br>Patients:'+
                                marker.patients
                                +'</strong></td></tr></table>' + '</div>');
          infowindow.open(map, marker);
        });
      }

      google.maps.event.addDomListener(window, 'load', initialize);
    </script>
  </head>
  <body>
    <div id="map"></div>
  </body>
</html>